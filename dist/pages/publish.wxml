
    <view class="publish-container">
        <view class="publish-input">
            <view>
                <image src="../images/{{themeimg}}title.png"/>
                <input type="text" placeholder="请输入发布内容的标题或主题" maxlength="100" placeholder-class="input-placeholder" bindinput="titleChange" value="{{title}}"/>
            </view>
        </view>
        <view class="publish-input">
            <view>
                <image src="../images/{{themeimg}}subtitle.png"/>
                <input type="text" placeholder="请输入关于发布内容的 2-3个关键词,空格分割" maxlength="20" placeholder-class="input-placeholder" bindinput="subTitleChange" value="{{subtitle}}"/>
            </view>
        </view>
        <view class="publish-input">
            <view>
                <image src="../images/{{themeimg}}phone.png"/>
                <input type="number" placeholder="请输入您的手机号" maxlength="11" placeholder-class="input-placeholder" value="{{phone}}" bindinput="phoneChange"/>
            </view>
        </view>
        <view class="publish-type">
            <view class="publish-notice">请选择发布所属分类</view>
            <view class="publish-tags">
                <view class="{{type=='job'?'active':''}}" bindtap="tap" data-wpytap-a="job"><text>招聘信息</text></view>
                <view class="{{type=='sale'?'active':''}}" bindtap="tap" data-wpytap-a="sale"><text>打折促销</text></view>
                <view class="{{type=='home'?'active':''}}" bindtap="tap" data-wpytap-a="home"><text>房屋租售</text></view>
                <view class="{{type=='car'?'active':''}}" bindtap="tap" data-wpytap-a="car"><text>车辆交易</text></view>
                <view class="{{type=='find'?'active':''}}" bindtap="tap" data-wpytap-a="find"><text>寻人寻物</text></view>
            </view>
            <view class="publish-tags">
                <view class="{{type=='food'?'active':''}}" bindtap="tap" data-wpytap-a="food"><text>美食推荐</text></view>
                <view class="{{type=='friend'?'active':''}}" bindtap="tap" data-wpytap-a="friend"><text>相亲交友</text></view>
                <view class="{{type=='pet'?'active':''}}" bindtap="tap" data-wpytap-a="pet"><text>宠物领养</text></view>
                <view class="{{type=='edu'?'active':''}}" bindtap="tap" data-wpytap-a="edu"><text>教育培训</text></view>
                <view class="{{type=='other'?'active':''}}" bindtap="tap" data-wpytap-a="other"><text>其他消息</text></view>
            </view>
        </view>
        <view class="publish-form">
            <view class="publish-notice">请填写相关信息 <text class="publish-tips">(<text>*</text>为必填项)</text></view>

